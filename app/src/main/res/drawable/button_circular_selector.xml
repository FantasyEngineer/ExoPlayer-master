<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 非触摸模式下取得焦点并单击时的背景图片 -->
    <item android:drawable="@drawable/button_circular_press" android:state_pressed="true"/>

    <!-- 选中时的背景图片 -->
    <item android:drawable="@drawable/button_circular_press" android:state_selected="true"/>

    <!-- 默认图片 -->
    <item android:drawable="@drawable/button_circular_normal" android:state_selected="false"/>


</selector>